<?xml version="1.0"?>
<launch>
    <arg name="start" default="0" />
    <include file="$(find rwrc23)/launch/common/default_full.launch">
        <!-- course -->
        <arg name="checkpoint" value="$(find rwrc23)/map/checkpoint/dkan_checkpoint.yaml"/>
        <arg name="task_list" value="$(find rwrc23)/map/task/dkan_task.yaml"/>

        <!-- common -->
        <arg name="start_node_id" value="$(arg start)"/>
        <arg name="stop_list" value="$(find rwrc23)/map/stop/dkan_stop.yaml"/>
        <arg name="use_stop_line_detector" value="true"/>
        <arg name="stop_line_param" default="$(find rwrc23)/config/param/stop_line_detector/dkan.yaml"/>
        <arg name="local_goal_dist" value="4.0"/>
        <arg name="local_map_expansion" value="0.075"/>
        <arg name="planner_hz" value="20"/>
        <!-- Localizer method -->
        <arg name="localizer_3d/localization_method" value="ndt"/> <!-- ndt or mcl_with_imu or mcl_no_imu-->
        <arg name="localizer_3d/use_static_clouds" value="false"/>
        <!-- map -->
        <arg name="map_pcd" value="/home/amsl/pcd/dkan_1f.pcd"/>
        <arg name="node_edge_map" value="$(find rwrc23)/map/graph/dkan_graph.yaml"/>
        <!-- rviz -->
        <arg name="rviz_conf" value="$(find rwrc23)/config/rviz/rviz.rviz"/>
    </include>
</launch>
